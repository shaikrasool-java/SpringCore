public class Collection {

	public static void main(String[] args) {
		
		Object obj=null;
		ApplicationContext ap=new ClassPathXmlApplicationContext("src/com/java/cfgs/collection.xml");
		obj=ap.getBean("c");
		CollectionsDI cdi=(CollectionsDI)obj;
	}
}
